div.page_links
    if pageNumbers[0] !== 1
        a(href=`/questions?page=${1}`) First page
    if currentPage !== 1){
        a(href=`/questions?page=${currentPage-1}`) Prev
    each page in pageNumbers
        a(href=`/questions?page=${page}`) #{page}
    if pageNumbers[pageNumbers.length-1] !== amountOfPages)
        a(href=`/questions?page=${amountOfPages}`) Last Page
    if(currentPage !== amountOfPages)
        a(href=`/questions?page=${currentPage+1}`) Next
